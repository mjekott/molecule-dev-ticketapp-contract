{
  "name": "@molecule-dev/ticketapp-contracts",
  "version": "1.0.4",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "type": "dist/index.d.ts",
  "files": [
    "dist",
    "proto",
    "gen"
  ],
  "description": "Protocol Buffer definitions and generated TypeScript code",
  "exports": {
    "./contracts/gen/auth": {
      "types": "./gen/auth.ts",
      "default": "./gen/auth.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "generate": "protoc   --proto_path=./proto   --ts_proto_out=./gen   --ts_proto_opt=nestJs=true,package=omit   ./proto/*.proto"
  },
  "dependencies": {
    "@nestjs/microservices": "^11.1.11",
    "rxjs": "^7.8.2",
    "ts-proto": "^2.10.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "typescript": "^5.9.3"
  }
}
